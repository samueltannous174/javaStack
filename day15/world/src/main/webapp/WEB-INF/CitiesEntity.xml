<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="org.example.world.Models.CitiesEntity">
        <table name="cities" schema="world" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name" length="35"/>
            </basic>
            <basic name="countryCode">
                <column name="country_code" length="3"/>
            </basic>
            <basic name="district">
                <column name="district" length="20"/>
            </basic>
            <basic name="population">
                <column name="population" precision="-1"/>
            </basic>
            <basic name="countryId">
                <column name="country_id" precision="-1"/>
            </basic>
            <many-to-one name="countriesByCountryId" optional="true"
                         target-entity="org.example.world.Models.CountriesEntity">
                <join-column name="country_id" referenced-column-name="id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
